{
  "kind": "collectionType",
  "collectionName": "portfolio_images",
  "info": {
    "name": "portfolio-images"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "category": {
      "model": "portfolio-categories"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "modalImages": {
      "collection": "modal-image"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "hoveredTextFontSizes": {
      "type": "enumeration",
      "enum": [
        "xs",
        "sm",
        "md",
        "lg",
        "xl"
      ]
    },
    "mainPositions": {
      "model": "portfolio-positions"
    },
    "categoryPositions": {
      "model": "portfolio-positions"
    }
  }
}
